sukupuoli = input("Mikä on biologinen sukupuoli? (nainen/mies): ")
hemoglobiini = float(input("Mikä on hemoglobiinin arvo (g/1)? "))

if sukupuoli == "mies":
    if hemoglobiini <134:
        print("hemoglobiinin arvo on alhainen.")
    elif hemoglobiini >167:
        print("hemoglobiini on korkea.")
    else:
        print("hemoglobiini on normaali-")
elif sukupuoli == "nainen":
    if hemoglobiini <117:
        print("hemoglobiini on alhainen.")
    elif hemoglobiini >155:
        print("hemoglobiini on korkea.")
    else:
        print("hemoglobiini on normaali.")